{
  "name": "swoft/amqp",
  "type": "library",
  "version": "v2.0.11",
  "keywords": [
    "php",
    "swoole",
    "swoft"
  ],
  "description": "micro-service framework base on swoole",
  "license": "Apache-2.0",
  "require": {
    "php": ">7.1",
    "php-amqplib/php-amqplib": "^2.9",
    "swoft/connection-pool": "~2.0.0",
    "swoft/stdlib": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Amqp\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Amqp\\Unit\\": "test/unit/",
      "SwoftTest\\Amqp\\Testing\\": "test/testing/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
